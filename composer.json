{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "3.3.20.1",
    "craftcms/contact-form": "2.2.6",
    "doublesecretagency/craft-smartmap": "3.2.2.1",
    "froala/craft-froala-wysiwyg": "2.9.5",
    "rynpsc/craft-phone-number": "1.2.5",
    "solspace/craft-freeform": "3.5.10",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
