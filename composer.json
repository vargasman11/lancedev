{
  "require": {
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "3.4.1",
    "craftcms/contact-form": "2.2.6",
    "craftcms/feed-me": "4.2.0.1",
    "craftcms/redactor": "2.5.0",
    "doublesecretagency/craft-smartmap": "3.2.2.1",
    "froala/craft-froala-wysiwyg": "3.1.0",
    "rynpsc/craft-phone-number": "1.3.0",
    "solspace/craft-freeform": "3.6.1",
    "verbb/redactor-tweaks": "2.0.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
